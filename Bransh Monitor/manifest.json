{
    "manifest_version": 3,
    "name": "Branch Monitor",
    "version": "1.0",
    "permissions": [
        "notifications"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "alert.mp3"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}